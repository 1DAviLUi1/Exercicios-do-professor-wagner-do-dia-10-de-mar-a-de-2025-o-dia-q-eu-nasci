#include <stdio.h>

int main() {
   int numero;
   int wike, tipoelafalaqehbruxa;
   wike = -1;
   
   printf("Digite seu numero q eu faco a tabuada deleat√©10: ");
   scanf(" %d", &numero);
   while(wike <10){
       wike++;
       tipoelafalaqehbruxa = wike * numero;
       printf(" %d X %d = %d\n", numero, wike, tipoelafalaqehbruxa);
   }
    printf(" :D");
   return 0;
}
