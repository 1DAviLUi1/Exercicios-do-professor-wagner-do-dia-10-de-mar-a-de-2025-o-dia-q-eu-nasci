// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int num;
    int mun;
    num = 8;
    
    printf("Adivinhe o numero que eu estou pensando\n");
    printf("Digite:");
    scanf(" %d", &mun);
    
    if (mun == 8){
    printf("Voce acertou O.o, parabens");
    return 0;
    }
    else{
        printf("Voce ta errado :P");
    }
}
