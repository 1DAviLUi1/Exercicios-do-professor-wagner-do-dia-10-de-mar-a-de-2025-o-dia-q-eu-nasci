// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n;

    printf("Digite seu numero");
    scanf(" %d", &n);
    if(n <0){
        printf("Numero é negativo");
    }
    else if(n >0){
        printf("o numero é Positivo");
    }
    else{
        printf(" o numero é 0");
    }
}
